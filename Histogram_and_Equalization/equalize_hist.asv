function [histo, cdf] = equalize_hist(img)
    histo = zeros(1,256);
    cdf = zeros(1,256);
    pixSum = 0;

    for i = 1:256
        hist(i) = sum(img(:) == i);
        pixSum = pixSum + hist(i);
        cdf(i) = pixSum;
    end

    hist = hist/max(cdf);
    cdf = cdf * 255/max(cdf);
    cdf = uint8(cdf);
end
